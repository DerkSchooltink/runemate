<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="189.0" prefHeight="189.0"
         prefWidth="457.0" type="StackPane" xmlns="http://javafx.com/javafx/9.0.1">
    <GridPane alignment="TOP_CENTER" prefHeight="138.0" prefWidth="457.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label prefHeight="17.0" prefWidth="91.0" text="Obelisk location:" GridPane.halignment="RIGHT">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </Label>
        <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="82.0" text="Start"
                GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="3"
                GridPane.valignment="CENTER"/>
        <ComboBox fx:id="obeliskComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.halignment="LEFT">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
        </ComboBox>
        <ComboBox fx:id="pouchComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.halignment="LEFT"
                  GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
        </ComboBox>
        <Label text="Pouch type:" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Preset:" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </Label>
        <ComboBox fx:id="presetComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.halignment="LEFT"
                  GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
        </ComboBox>
    </GridPane>
</fx:root>
