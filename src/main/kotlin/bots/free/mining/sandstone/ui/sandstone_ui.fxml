<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="150.0" prefHeight="150.0"
         prefWidth="457.0" type="StackPane" xmlns="http://javafx.com/javafx/9.0.1">
    <GridPane alignment="TOP_CENTER" prefHeight="138.0" prefWidth="457.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label prefHeight="17.0" prefWidth="116.0" text="Use juju potions:"/>
        <CheckBox fx:id="jujuCheckBox" mnemonicParsing="false" GridPane.columnIndex="1"/>
        <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="82.0" text="Start"
                GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                GridPane.valignment="CENTER"/>
        <CheckBox fx:id="catchButterfliesCheckBox" mnemonicParsing="false" GridPane.columnIndex="1"
                  GridPane.rowIndex="1"/>
        <Label text="Catch Guthixian butterflies:" GridPane.rowIndex="1"/>
    </GridPane>
</fx:root>
