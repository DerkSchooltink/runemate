<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" maxHeight="80.0" maxWidth="400.0" prefHeight="82.0" prefWidth="399.0"
         type="StackPane" xmlns="http://javafx.com/javafx/9.0.1">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" text="Start"
                GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                GridPane.valignment="CENTER"/>
        <Slider fx:id="presetSlider" majorTickUnit="1.0" max="10.0" maxWidth="335.0" min="1.0" minWidth="100.0"
                minorTickCount="0" prefHeight="38.0" prefWidth="161.0" showTickLabels="true" showTickMarks="true"
                snapToTicks="true" GridPane.columnIndex="1"/>
        <Label text="Preset:"/>
    </GridPane>
</fx:root>
