<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
         minWidth="-Infinity" prefHeight="160.0" prefWidth="508.0" type="StackPane"
         xmlns="http://javafx.com/javafx/9.0.1">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Start"
                GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                GridPane.valignment="CENTER"/>
        <CheckBox fx:id="twoTickCheckBox" mnemonicParsing="false" GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
            <tooltip>
                <Tooltip text="Clicks the memory every 2 game ticks for faster exp."/>
            </tooltip>
        </CheckBox>
        <Label text="Enable two-ticking" GridPane.halignment="RIGHT">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
            <tooltip>
                <Tooltip text="Clicks the memory every 2 game ticks for faster exp."/>
            </tooltip>
        </Label>
    </GridPane>
</fx:root>
