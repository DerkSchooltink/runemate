<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="141.0" prefWidth="360.0" type="StackPane"
         xmlns="http://javafx.com/javafx/9.0.1">
    <GridPane prefHeight="102.0" prefWidth="375.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <ComboBox fx:id="prayerPotionComboBox" prefWidth="150.0" GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
        </ComboBox>
        <ComboBox fx:id="overloadComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
        </ComboBox>
        <Label text="Prayer potion:" GridPane.halignment="RIGHT">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Overload:" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </Label>
        <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Start"
                GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
    </GridPane>
</fx:root>
