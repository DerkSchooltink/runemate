<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="StackPane" xmlns="http://javafx.com/javafx/9.0.1">
    <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="333.0"
          prefWidth="389.0">
        <VBox prefHeight="200.0" prefWidth="100.0">
            <GridPane prefHeight="200.0" prefWidth="428.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <RadioButton fx:id="cookingRadioButton" contentDisplay="CENTER" mnemonicParsing="false" text="Cooking"
                             textAlignment="CENTER"/>
                <RadioButton fx:id="preciousOreRadioButton" alignment="BOTTOM_RIGHT" contentDisplay="BOTTOM"
                             mnemonicParsing="false" text="Precious ore smithing" textAlignment="RIGHT"
                             GridPane.rowIndex="1"/>
                <RadioButton fx:id="oreRadioButton" mnemonicParsing="false" text="Ore smithing" GridPane.rowIndex="4"/>
                <RadioButton fx:id="summoningRadioButton" mnemonicParsing="false" text="Summoning"
                             GridPane.columnIndex="1"/>
                <RadioButton fx:id="oreRockRadioButton" mnemonicParsing="false" text="Ore mining"
                             GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <RadioButton fx:id="preciousOreMiningRadioButton" mnemonicParsing="false" text="Precious ore mining"
                             GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <RadioButton fx:id="woodcuttingRadioButton" mnemonicParsing="false" text="Woodcutting"
                             GridPane.rowIndex="2"/>
                <RadioButton fx:id="firemakingRadioButton" mnemonicParsing="false" text="Firemaking"
                             GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <RadioButton fx:id="weaveRadioButton" mnemonicParsing="false" text="Weaving" GridPane.rowIndex="3"/>
                <RadioButton fx:id="stoneOreRadioButton" mnemonicParsing="false" text="Stone mining"
                             GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            </GridPane>
        </VBox>
        <GridPane prefHeight="133.0" prefWidth="389.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <CheckBox fx:id="anagogicOrtCheckBox" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                      text="Collect anagogic ort" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
            <Separator prefHeight="6.0" prefWidth="389.0" GridPane.valignment="BOTTOM"/>
            <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="83.0" text="Start"
                    GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
        </GridPane>
    </VBox>
</fx:root>
