<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
         minWidth="-Infinity" prefHeight="197.0" prefWidth="462.0" type="StackPane"
         xmlns="http://javafx.com/javafx/9.0.1">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <ComboBox fx:id="intermediateTaskComboBox" prefWidth="150.0" GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </ComboBox>
        <Label text="Task to do in between thieving:" GridPane.halignment="RIGHT">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="98.0" text="Start"
                GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                GridPane.valignment="CENTER"/>
        <Label text="Use crystal mask spell:" GridPane.halignment="RIGHT" GridPane.rowIndex="1"
               GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <CheckBox fx:id="crystalMaskCheckBox" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </CheckBox>
    </GridPane>
</fx:root>
