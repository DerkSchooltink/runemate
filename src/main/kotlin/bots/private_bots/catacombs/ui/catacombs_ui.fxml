<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="314.0" prefWidth="488.0" type="StackPane"
         xmlns="http://javafx.com/javafx/9.0.1">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="89.0" text="Start"
                GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="7"/>
        <ComboBox fx:id="monsterComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.halignment="LEFT"
                  GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </ComboBox>
        <Label text="Monster:" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <ComboBox fx:id="foodComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                  GridPane.rowIndex="3" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </ComboBox>
        <Label text="Food:" GridPane.halignment="RIGHT" GridPane.rowIndex="3" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Attack potion:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <ComboBox fx:id="attackPotionComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                  GridPane.rowIndex="1" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </ComboBox>
        <Spinner fx:id="attackPotionAmountSpinner" editable="true" prefHeight="25.0" prefWidth="65.0"
                 GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </Spinner>
        <Spinner fx:id="foodAmountSpinner" editable="true" prefHeight="25.0" prefWidth="65.0" GridPane.columnIndex="1"
                 GridPane.halignment="LEFT" GridPane.rowIndex="3" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </Spinner>
        <ComboBox fx:id="strengthPotionComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                  GridPane.rowIndex="2" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </ComboBox>
        <Spinner fx:id="strengthPotionAmountSpinner" editable="true" prefWidth="65.0" GridPane.columnIndex="1"
                 GridPane.halignment="LEFT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </Spinner>
        <Label text="Strength potion:" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Minimum loot price:" GridPane.halignment="RIGHT" GridPane.rowIndex="4"
               GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Minimum stack loot price:" GridPane.halignment="RIGHT" GridPane.rowIndex="5"
               GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <Spinner fx:id="lootPriceSpinner" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="4">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </Spinner>
        <Spinner fx:id="lootStackPriceSpinner" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="5">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </Spinner>
        <Label text="Special attack percentage needed:" GridPane.halignment="RIGHT" GridPane.rowIndex="6"
               GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Label>
        <Spinner fx:id="specialAttackPercentageSpinner" editable="true" prefHeight="25.0" prefWidth="65.0"
                 GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="6"
                 GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="5.0"/>
            </GridPane.margin>
        </Spinner>
        <CheckBox fx:id="specialAttackCheckBox" mnemonicParsing="false" text="Use special attack"
                  GridPane.columnIndex="1" GridPane.rowIndex="6">
            <GridPane.margin>
                <Insets left="5.0"/>
            </GridPane.margin>
        </CheckBox>
    </GridPane>
</fx:root>
