<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="254.0" prefWidth="488.0" type="StackPane"
         xmlns="http://javafx.com/javafx/9.0.1">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="220.0" minHeight="10.0" prefHeight="190.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="122.0" minHeight="10.0" prefHeight="64.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <ListView fx:id="usernameListView" prefHeight="195.0" prefWidth="244.0" GridPane.columnSpan="2"/>
        <Button fx:id="startButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="122.0" text="Start"
                GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
        <GridPane GridPane.columnIndex="1" GridPane.rowIndex="1">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="63.0" minWidth="10.0" prefWidth="63.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="114.0" minWidth="10.0" prefWidth="71.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="118.0" minWidth="10.0" prefWidth="104.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <RadioButton fx:id="f2pRadioButton" mnemonicParsing="false" text="F2P"/>
            <RadioButton fx:id="p2pRadioButton" mnemonicParsing="false" text="P2P" GridPane.columnIndex="1"/>
            <RadioButton fx:id="noHoppingRadioButton" mnemonicParsing="false" text="Don't hop"
                         GridPane.columnIndex="2"/>
        </GridPane>
    </GridPane>
</fx:root>
